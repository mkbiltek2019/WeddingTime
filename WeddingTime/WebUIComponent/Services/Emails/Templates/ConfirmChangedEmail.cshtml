@model AIT.WebUIComponent.Models.Emails.SystemMessage

<!DOCTYPE html>

<html lang="en" xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
</head>
<body>
    <p>
        <img src="cid:@Model.ContentId" />
    </p>
    <p>
        Witaj @Model.Name.
    </p>
    <p>
        Adres e-mail w serwisie <a href="https://zamilowani.pl">Zamiłowani.pl</a> został zmieniony.
    </p>
    <p>
        Aktywuj swoje konto klikając w ten <a href="@Model.CallbackUrl">link</a>.
    </p>
    <p>
        <span>Zespół </span><span>Zamiłowani</span><span>.pl</span>
    </p>
</body>
</html>  